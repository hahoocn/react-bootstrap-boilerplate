{
  "name": "react-bootstrap-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate app for bootstrap with React",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=production babel-node src/server/main.js",
    "start:dev": "cross-env NODE_ENV=development babel-node src/server/main.js",
    "start:hot": "cross-env NODE_ENV=development babel-node webpack/webpack.devServer.js",
    "dev": "npm run start:hot & npm run start:dev",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --progress",
    "build": "cross-env NODE_ENV=production webpack --verbose --colors --display-error-details --progress --config webpack/webpack.config.server.js",
    "profile": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --profile --json > profile.json",
    "clean": "rimraf build && rimraf public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hahoocn/react-bootstrap-boilerplate.git"
  },
  "keywords": [
    "react",
    "bootstrap",
    "redux",
    "boilerplate"
  ],
  "author": "Ray Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hahoocn/react-bootstrap-boilerplate/issues"
  },
  "homepage": "https://github.com/hahoocn/react-bootstrap-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "bootstrap-loader": "^1.0.10",
    "bootstrap-sass": "^3.3.6",
    "clean-webpack-plugin": "^0.1.9",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.3",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "font-awesome-loader": "0.0.1",
    "handlebars-loader": "^1.3.0",
    "html-webpack-plugin": "^2.19.0",
    "image-webpack-loader": "^1.8.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "postcss-mixins": "^4.0.2",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^2.0.0",
    "react-hot-loader": "^1.3.0",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "hahoorequest": "^0.5.0",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-bootstrap": "^0.23.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-act-reducer": "^1.4.1",
    "redux-thunk": "^2.1.0",
    "webpack-isomorphic-tools": "^2.3.2"
  }
}
