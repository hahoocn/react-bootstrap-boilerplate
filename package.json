{
  "name": "react-bootstrap-boilerplate",
  "version": "0.1.3",
  "description": "Boilerplate app for bootstrap with React",
  "scripts": {
    "start": "cross-env NODE_ENV=production babel-node src/server/main.js",
    "start:dev": "cross-env NODE_ENV=development babel-node src/server/main.js",
    "start:hot": "cross-env NODE_ENV=development babel-node webpack/webpack.devServer.js",
    "dev": "concurrently --kill-others \"npm run start:hot\" \"npm run start:dev\"",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --progress",
    "build": "cross-env NODE_ENV=production webpack --verbose --colors --display-error-details --progress --config webpack/webpack.config.server.js",
    "profile": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --profile --json > profile.json",
    "clean": "rimraf build && rimraf public",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hahoocn/react-bootstrap-boilerplate.git"
  },
  "keywords": [
    "react",
    "bootstrap",
    "redux",
    "boilerplate"
  ],
  "author": "Ray Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hahoocn/react-bootstrap-boilerplate/issues"
  },
  "homepage": "https://github.com/hahoocn/react-bootstrap-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "bootstrap-loader": "^1.3.3",
    "bootstrap-sass": "^3.3.7",
    "clean-webpack-plugin": "^0.1.14",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "^1.0.1",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.13.1",
    "postcss": "^5.2.8",
    "postcss-loader": "^1.2.1",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.0",
    "stylelint-config-standard": "^15.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "concurrently": "^3.1.0",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "hahoorequest": "^0.6.0",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-helmet": "^3.2.3",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-act-reducer": "^1.5.2",
    "redux-thunk": "^2.1.0",
    "webpack-isomorphic-tools": "^2.6.5"
  }
}
